<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Category" table="category">
    <id name="idCategory" type="integer" column="id_category">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="text" column="name" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="titleSeo" type="text" column="title_seo" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionShort" type="text" column="description_short" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="text" column="description" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="linkRewrite" type="string" column="link_rewrite" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAdd" type="datetime" column="date_add" nullable="true"/>
    <field name="dateUpd" type="datetime" column="date_upd" nullable="true"/>
  </entity>
</doctrine-mapping>
