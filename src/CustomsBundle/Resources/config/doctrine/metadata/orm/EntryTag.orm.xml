<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EntryTag" table="entry_tag">
    <indexes>
      <index name="fk_entry_tag_entry" columns="id_entry"/>
      <index name="fk_entry_tag_tag" columns="id_tag"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="idEntry" target-entity="Entry" fetch="LAZY">
      <join-columns>
        <join-column name="id_entry" referenced-column-name="id_entry"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idTag" target-entity="Tag" fetch="LAZY">
      <join-columns>
        <join-column name="id_tag" referenced-column-name="id_tag"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
