<?php

namespace CustomsBundle\Entity;

/**
 * EntryTag
 */
class EntryTag
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var \CustomsBundle\Entity\Entry
     */
    private $idEntry;

    /**
     * @var \CustomsBundle\Entity\Tag
     */
    private $idTag;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idEntry
     *
     * @param \CustomsBundle\Entity\Entry $idEntry
     *
     * @return EntryTag
     */
    public function setIdEntry(\CustomsBundle\Entity\Entry $idEntry = null)
    {
        $this->idEntry = $idEntry;

        return $this;
    }

    /**
     * Get idEntry
     *
     * @return \CustomsBundle\Entity\Entry
     */
    public function getIdEntry()
    {
        return $this->idEntry;
    }

    /**
     * Set idTag
     *
     * @param \CustomsBundle\Entity\Tag $idTag
     *
     * @return EntryTag
     */
    public function setIdTag(\CustomsBundle\Entity\Tag $idTag = null)
    {
        $this->idTag = $idTag;

        return $this;
    }

    /**
     * Get idTag
     *
     * @return \CustomsBundle\Entity\Tag
     */
    public function getIdTag()
    {
        return $this->idTag;
    }
}

