
<!DOCTYPE HTML>
<html lang="es">
<head>
    <meta charset="utf-8"/>
    <title>
        {% block title %} NutriK {% endblock %}
    </title>
    <!-- Login Page stylesheet-->
    <link rel="stylesheet" href="{{ asset('bundles/framework/css/loginpage.css') }}" />
</head>
<body>
{% block content %}
    <div class="container">
        <div class="row title">
            <h1>NutriK</h1>
        </div>
        <div class="row logincarousel">
            <div class="col-sm-12">
                {% include 'carousel.html.twig' %}
            </div>
        </div>
        {% if status == "OK" %}
            
        {% endif %}

        {% if status == "KO" %}
            <div class="row">
                <div class="col-sm-6">
                    <div class="alert alert-danger" role="alert">
                        {{ error }}
                    </div>
                </div>
            </div>
        {% endif %}

        <div class="row registerform">
            <div class="col-sm-6">
                <h2>Registrate</h2>
                {{ form_start(form) }}
                {{ form_end(form) }}
            </div>
        </div>
    </div>
    <div class="clearfix"></div>
{% endblock %}
</body>

